{
  "name": "@mavrou/cy-graphql",
  "version": "2.3.0",
  "description": "Cypress custom commands for GraphQL testing",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "/dist/*"
  ],
  "scripts": {
