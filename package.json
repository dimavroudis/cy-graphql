    "cypress": "15.9.0",
    "express": "^5.1.0",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "start-server-and-test": "2.1.3",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "cypress": ">=3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimavroudis/cy-graphql.git"
  },
  "bugs": {
